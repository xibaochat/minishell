 WITH [SETUP echo bonjour > test] cat < test
 WITH [SETUP echo bonjour > test'sticked'] cat < test'sticked'
 WITH [SETUP echo bonjour > test'sticked'] < test'sticked' cat
 WITH [SETUP echo bonjour > test"sticked"] cat < test"sticked"
 WITH [SETUP echo bonjour > test"sticked"] < test"sticked" cat
 WITH [SETUP echo bonjour > test'yo'"sticked"] cat < test'yo'"sticked"
 WITH [SETUP echo bonjour > test'yo'"sticked"] < test'yo'"sticked" cat
 WITH [SETUP echo bonjour > test; echo salut > je] cat < test < je
 WITH [SETUP echo bonjour > test; echo salut > je] cat<test<je
 WITH [SETUP echo bonjour > file] < file
 WITH <
 WITH echo <
 WITH [SETUP touch test] < test
 WITH [SETUP echo bonjour > bar] cat << < bar
 WITH [SETUP echo bonjour > bar] cat << << bar
 WITH [SETUP echo bonjour > bar] cat <<<<< bar
 WITH cat < doesnotexist
 WITH cat < doesntexist; echo $?
 WITH [SETUP echo bonjour > noperm; chmod 000 noperm] cat < noperm; echo $?
 WITH cat < doesntexist
 WITH [SETUP echo bonjour > noperm; chmod 000 noperm] cat < noperm
 WITH [SETUP touch a b c d; mkdir m1 m2 m3; echo bonjour > a] ls -l | cat -e < a
 WITH [SETUP echo b > foo] echo a | cat -e < foo
 WITH [SETUP echo b > foo] echo a > bar | cat -e < foo
 WITH < | a
 WITH < ; a
 WITH ; < a
 WITH | < a
 WITH < a ; a
