 WITH >> test'yo'"sticked" echo bonjour
 WITH [SETUP echo bonjour > test'yo'"sticked"] cat < test'yo'"sticked"
 WITH [SETUP echo bonjour > test'yo'"sticked"] < test'yo'"sticked" cat
 WITH echo bonjour > test > je > suis
 WITH echo > test > je bonjour > suis
 WITH > test echo bonjour > je > suis
 WITH echo bonjour >> test > je >> suis
 WITH echo >> test bonjour > je > suis
 WITH >> test echo > je bonjour > suis
 WITH [SETUP echo bonjour > test; echo salut > je] cat < test < je
 WITH echo bonjour>test>je>suis
 WITH >test echo bonjour>je>suis
 WITH echo bonjour>>test>je>>suis
 WITH [SETUP echo bonjour > test; echo salut > je] cat<test<je
