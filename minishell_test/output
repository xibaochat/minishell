 WITH echo bonjour > test
 WITH echo > test bonjour
 WITH > test echo bonjour
 WITH echo bonjour >> test
 WITH echo >> test bonjour
 WITH >> test echo bonjour
 WITH [SETUP echo bonjour > test] cat < test
 WITH echo bonjour > test
 WITH echo > test'sticked' bonjour
 WITH > test'sticked' echo bonjour
 WITH echo bonjour >> test'sticked'
 WITH echo >> test'sticked' bonjour
 WITH >> test'sticked' echo bonjour
