 WITH [EXPORTS PATH=''] [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd] somecmd
 WITH [EXPORTS PATH=':'] [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd] somecmd
 WITH [EXPORTS PATH='::::::::'] [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd] somecmd
 WITH [EXPORTS PATH='/asdfasdf'] [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd] somecmd
 WITH [EXPORTS PATH='/usr/asdf:/usr/lib/a'] [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd] somecmd
 WITH [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd; unset PATH] somecmd
 WITH [EXPORTS PATH='/bin:'] [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd] somecmd
 WITH [EXPORTS PATH=':/bin'] [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd] somecmd
 WITH [EXPORTS PATH=':/bin:'] [SETUP echo '#!/bin/sh\necho bonjour' > somecmd; chmod +x somecmd] somecmd
